   "source": [
    "Question 1: What data type is each of the following?\n",
    "5 = INT\n",
    "5.0 = FLOATING POINT NUMBER\n",
    "5 > 1(TRUE) = BOOLEAN\n",
    "'5' = STRING\n",
    "5 * 2 (10) = INT \n",
    "'5' * 2 (106) = INT\n",
    "'5' + '2' (52) = STRING\n",
    "5 / 2 = FLOAT\n",
    "5 % 2 = FLOAT\n",
    "{5, 2, 1} = DICTIONARY\n",
    "5 == 3 (FALSE) = BOOLEAN\n",
    "Pi (the number) = PI"
